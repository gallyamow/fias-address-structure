{
  "name": "addresser/fias-address-structure",
  "version": "2.0.1",
  "description": "FIAS-address representation",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-mbstring": "*",
    "webmozart/assert": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Addresser\\FiasAddressStructure\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Addresser\\FiasAddressStructure\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.6"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "composer-validate": "@composer validate --strict",
    "cs": "php-cs-fixer fix -v --dry-run --ansi",
    "cs-fix": "php-cs-fixer fix -v --diff --ansi"
  }
}
